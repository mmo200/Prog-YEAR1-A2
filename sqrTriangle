def outputTriangle():
    userIn = int(input("Please Enter in the Hieght of the Triangle: "))
    print("*")
    for height in range(userIn - 2 ):
        print('*', end= 'a')
        for space in range(height):
            print('s', end= 'b')
        print('*')
    
main()
